{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "OopisOS",
  "version": "4.1.0",
  "identifier": "com.oopisos.oop",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devUrl": "https://github.com/aedmark/oopis-os-dev",
    "frontendDist": "../public"
  },
  "app": {
    "windows": [
      {
        "title": "OopisOS",
        "width": 1024,
        "height": 768,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["app"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}